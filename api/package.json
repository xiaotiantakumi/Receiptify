{
    "name": "pwa-starter-api",
    "version": "1.0.0",
    "description": "Azure Functions API for PWA Starter Template",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "prestart": "npm run build",
        "start": "func start",
        "predev": "npm run watch &",
        "dev": "func start --verbose",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@azure/data-tables": "^13.3.1",
        "@azure/functions": "^4.0.0",
        "@azure/storage-blob": "^12.28.0",
        "@google/generative-ai": "^0.24.1",
        "@types/uuid": "^10.0.0",
        "dayjs": "^1.11.13",
        "uuid": "^11.1.0",
        "zod": "^4.0.14"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/jsdom": "^21.1.6",
        "@types/node": "^20.11.24",
        "azure-functions-core-tools": "^4.0.0",
        "jest": "^29.7.0",
        "jest-mock-extended": "^3.0.5",
        "ts-jest": "^29.1.2",
        "typescript": "^5.3.3"
    }
}